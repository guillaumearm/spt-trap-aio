{
    "name": "trap-aio",
    "displayName": "Trap's AIO",
    "version": "0.5.0",
    "main": "src/mod.js",
    "license": "MIT",
    "author": "Trap",
    "akiVersion": ">=3.0.0",
    "scripts": {
        "build:unzipped": "copyfiles -e \"./node_modules/**/*.*\" -e \"./dist/**/*.*\" -e \"./package-lock.json\" -e \"./tsconfig.json\" -e \"./tmp/**/*.*\" -e \"./README.md\" -e \"./mod.code-workspace\" \"./**/*.*\" ./dist",
        "zip:files": "cd dist/ && bestzip ../mod.zip *",
        "build:zip": "npm run clean && npm run build:unzipped && npm run zip:files",
        "clean": "rimraf mod.zip dist tmp 'src/**/*.js'",
        "lint": "eslint --ignore-pattern '*.js' .",
        "build": "tsc",
        "test:all": "npm run clean && npm run build && npm run lint && npm run test",
        "test": "echo TODO: unit tests"
    },
    "devDependencies": {
        "@types/node": "^14.15.3",
        "@types/winston": "^2.4.4",
        "@typescript-eslint/eslint-plugin": "^4.33.0",
        "@typescript-eslint/parser": "^4.33.0",
        "bestzip": "2.2.1",
        "copyfiles": "2.4.1",
        "eslint": "7.26.0",
        "rimraf": "^3.0.2",
        "tsyringe": "4.6.0",
        "typescript": "4.6.4",
        "winston": "^3.7.2"
    }
}
